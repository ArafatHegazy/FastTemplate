@{
	TemplateEngine.DataStore["CurrentJob"] = "dev";
	TemplateFile.ProcessFileTemplate(TemplateEngine.CurrentTemplatePath + "\\" + "sample.template", TemplateEngine.CurrentOutputPath + "\\jenkins.dev",Model);
	TemplateEngine.DataStore["CurrentJob"] = "release";
	TemplateFile.ProcessFileTemplate(TemplateEngine.CurrentTemplatePath + "\\" + "sample.template", TemplateEngine.CurrentOutputPath + "\\jenkins.release",Model);
	
	TemplateFolder.ProcessFolderTemplate(TemplateEngine.CurrentTemplatePath + "\\" + "folder1.template", TemplateEngine.CurrentOutputPath + "\\Folder1",Model.Modules[0]);
	TemplateFolder.ProcessFolderTemplate(TemplateEngine.CurrentTemplatePath + "\\" + "folder1.template", TemplateEngine.CurrentOutputPath + "\\Folder2",Model.Modules[1]);
	TemplateFolder.ProcessFolderTemplate(TemplateEngine.CurrentTemplatePath + "\\" + "folder1.template", TemplateEngine.CurrentOutputPath + "\\Folder3",Model.Modules[2]);
	
}